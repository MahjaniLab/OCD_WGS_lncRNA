        logrr    = np.log(rr)
        lo95     = np.exp(logrr - z*se_logrr)
        hi95     = np.exp(logrr + z*se_logrr)
    else:
        lo95, hi95 = np.nan, np.nan

    ci_lower.append(rr - lo95)
    ci_upper.append(hi95 - rr)

