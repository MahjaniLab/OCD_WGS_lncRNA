import hail as hl

def collect_per_bin_arrays(ht: hl.Table):
    # (Important) This must be a Hail Table, not a pandas DataFrame.
    # Aggregate case/control sums and variant counts per gene
    gene_cc = ht.group_by(
        ht.LOEUF_bin, ht.loeuf_bin_start, ht.loeuf_bin_end, ht.Gene
    ).aggregate(
        case_sum    = hl.agg.sum(hl.int64(ht.case)),
        control_sum = hl.agg.sum(hl.int64(ht.control)),
        var_count   = hl.agg.count()
    )

    # Group into sorted arrays per bin (use a field name other than 'items' to avoid warnings)
    per_bin = gene_cc.group_by(
        gene_cc.LOEUF_bin, gene_cc.loeuf_bin_start, gene_cc.loeuf_bin_end
    ).aggregate(
        recs = hl.sorted(
            hl.agg.collect(
                hl.struct(g=gene_cc.Gene,
                          cs=gene_cc.case_sum,
                          ct=gene_cc.control_sum,
                          nv=gene_cc.var_count)
            ),
            key = lambda x: x.g  # Stable sort by gene name
        )
    ).order_by('loeuf_bin_start')

    # Collect to Python
    rows = per_bin.collect()

    # Convert to a Python-friendly structure
    bin_order = []  # Fix plotting order
    bin_payload = {}  # {bin_label: {'case': np.array, 'ctrl': np.array, 'nvar': np.array, 'genes': list}}
    for r in rows:
        label = r.LOEUF_bin
        bin_order.append((float(r.loeuf_bin_start), float(r.loeuf_bin_end), label))
        genes = [x.g for x in r.recs]
        cs    = np.array([int(x.cs) for x in r.recs], dtype=np.int64)
        ct    = np.array([int(x.ct) for x in r.recs], dtype=np.int64)
        nv    = np.array([int(x.nv) for x in r.recs], dtype=np.int64)
        bin_payload[label] = dict(genes=genes, case=cs, ctrl=ct, nvar=nv)

    # bin_order: list sorted by bin start value
    bin_order.sort(key=lambda t: t[0])
    return bin_order, bin_payload

from scipy import stats
import numpy as np

def rr_ci_and_p(cs_bin, ct_bin, n_case_total, n_ctrl_total, p0):
    # cs_bin, ct_bin: case/control counts summed over K genes sampled (with replacement) in one bin
    tot = cs_bin + ct_bin
    # Proportions
    p1 = cs_bin / n_case_total if n_case_total > 0 else np.nan
    p2 = ct_bin / n_ctrl_total if n_ctrl_total > 0 else np.nan
    rr = (p1 / p2) if (p2 > 0) else np.nan

    # Binomial test: check if case proportion equals p0 (overall case proportion)
    pval = stats.binomtest(k=cs_bin, n=tot, p=p0).pvalue if tot > 0 else 1.0

    # 95% CI (log scale, add 0.5 to avoid zeros)
    cs_cc = cs_bin if cs_bin > 0 else 0.5
    ct_cc = ct_bin if ct_bin > 0 else 0.5
    p1_cc = cs_cc / n_case_total if n_case_total > 0 else np.nan
    p2_cc = ct_cc / n_ctrl_total if n_ctrl_total > 0 else np.nan
    if p1_cc > 0 and p2_cc > 0:
        se_logp1 = np.sqrt((1 - p1_cc) / (n_case_total * p1_cc))
        se_logp2 = np.sqrt((1 - p2_cc) / (n_ctrl_total * p2_cc))
        se_logrr = np.sqrt(se_logp1**2 + se_logp2**2)
        logrr    = np.log(p1_cc / p2_cc)
        z        = 1.96
        lo95     = np.exp(logrr - z * se_logrr)
        hi95     = np.exp(logrr + z * se_logrr)
    else:
        lo95, hi95 = np.nan, np.nan
    return rr, lo95, hi95, pval

def run_bootstrap_with_replacement(bin_order, bin_payload, K=10, n_iter=200,
                                   n_case_total=2561, n_ctrl_total=12974, base_seed=20251107):
    rng = np.random.default_rng(base_seed)
    p0  = n_case_total / (n_case_total + n_ctrl_total)

    # Structure to store results: {bin_label: {'RR':[], 'p':[]}}
    out = {label: {'RR': [], 'p': []} for _, _, label in bin_order}

    for it in range(n_iter):
        # For each iteration, advance the RNG state
        # (rng is stateful, so no extra handling is needed)
        for _, _, label in bin_order:
            payload = bin_payload[label]
            n_genes = len(payload['genes'])
            if n_genes == 0:
                out[label]['RR'].append(np.nan)
                out[label]['p'].append(1.0)
                continue

            # Indices for sampling with replacement (duplicates allowed!)
            idx = rng.integers(0, n_genes, size=K)  # with replacement

            cs_bin = int(payload['case'][idx].sum())
            ct_bin = int(payload['ctrl'][idx].sum())

            rr, lo95, hi95, pval = rr_ci_and_p(cs_bin, ct_bin,
                                               n_case_total, n_ctrl_total, p0)
            out[label]['RR'].append(rr)
            out[label]['p'].append(pval)
    return out



import matplotlib.pyplot as plt
import numpy as np

def violin_plots_combined(out, bin_order, png_path="lncRNA_OCD_LOEUF_Bootstrap.png",
                          title_suffix="(bootstrap K with replacement)"):
    # Collect bin order and data
    labels  = [label for _, _, label in bin_order]
    rr_data = [out[label]['RR'] for label in labels]
    pv_data = [out[label]['p']  for label in labels]

    # Transform p-values to -log10 (clip to avoid zeros)
    pv_neglog10 = [
        -np.log10(np.clip(np.asarray(v, dtype=float), 1e-300, 1.0))
        for v in pv_data
    ]

    # Create figure (2 rows, 1 column)
    fig, axes = plt.subplots(2, 1, figsize=(14, 8), constrained_layout=False, sharex=False)

    # -------------------- (a) RR violin plot --------------------
    ax = axes[0]
    parts_rr = ax.violinplot(rr_data, showmeans=True, showextrema=False)
    # Set colors (RR = blue-ish)
    for pc in parts_rr['bodies']:
        pc.set_facecolor('#4C72B0')
        pc.set_edgecolor('black')
        pc.set_alpha(0.85)
    parts_rr['cmeans'].set_color('black')

    ax.set_xticks(np.arange(1, len(labels)+1))
    ax.set_xticklabels(labels, rotation=45, ha='right')
    ax.set_ylabel("Relative Risk (case rate / control rate)")
    ax.axhline(1.0, linestyle='--', color='red', linewidth=1)
    # Panel (a) label
    ax.text(-0.06, 1.04, "a", transform=ax.transAxes, fontsize=16, fontweight='bold', va='bottom', ha='left')

    # -------------------- (b) P-value violin plot --------------------
    ax = axes[1]
    parts_p = ax.violinplot(pv_neglog10, showmeans=True, showextrema=False)
    # Set colors (P-value = orange-ish)
    for pc in parts_p['bodies']:
        pc.set_facecolor('#DD8452')
        pc.set_edgecolor('black')
        pc.set_alpha(0.85)
    parts_p['cmeans'].set_color('black')

    ax.set_xticks(np.arange(1, len(labels)+1))
    ax.set_xticklabels(labels, rotation=45, ha='right')
    ax.set_ylabel(r"$-\mathrm{Log}_{10}(\mathit{P})$")
    # Reference line at p = 0.05
    ax.axhline(-np.log10(0.05), linestyle='--', color='red', linewidth=1)
    # Panel (b) label
    ax.text(-0.06, 1.04, "b", transform=ax.transAxes, fontsize=16, fontweight='bold', va='bottom', ha='left')

    # Common x-axis label
    try:
        fig.supxlabel("LOEUF score bin")
    except AttributeError:
        # For older matplotlib versions: attach to the bottom axis
        axes[1].set_xlabel("LOEUF score bin")

    # Adjust margins and save
    # fig.suptitle(f"RR & Binomial test P-value by LOEUF bin {title_suffix}", y=0.99, fontsize=14)
    fig.tight_layout(rect=[0, 0, 1, 0.97])  # avoid overlap with a potential suptitle
    fig.savefig(png_path, dpi=300, bbox_inches='tight')
    plt.close(fig)
    return png_path


